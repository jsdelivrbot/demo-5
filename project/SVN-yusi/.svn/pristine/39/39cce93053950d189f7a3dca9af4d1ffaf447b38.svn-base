#container {
    background: #F0F6F6;
    width:100%;
}
#container #left {
    width: 210px;
    height: calc(100% - 135px);
    background-color: #374050;
    float: left;
}
#container #left .left-item{
	height: 81px;
	line-height:81px ;
	color: #A9ACB1;
    text-align: center;
    font-size: 14px;
    cursor: pointer;
}
#container #left .left-item.active{
	color:#fff;
	background:#5D6371;
}

#container #right {
	padding:0px 73px 20px 56px;
  	margin-top:10px;
    width: calc(100% - 220px);
    height: calc(100% - 135px);
    float: right;
    background:#fff;

}
#container #right .right-title{
	height:82px;
	line-height:82px;
	text-align:center;
}

#container #right .right-title .create{
	width:120px;
	height:34px;
	line-height:34px;
    color: #fff;
    background-color: #4991EA;
    border-radius: 3px;
    border: none;
    margin-top:22px;
    margin-right: 10px;
}
#container #right .right-body-table .right-table-title{
	 height: 48px;
    line-height: 48px;
    background:#F2F2F2;


}
#container #right .right-body-table .right-table-title li{
    float: left;
    width: 155px;
    text-align: center;
    color: #696969;
    border-top: 1px solid #e5e5e5;
    border-left: 1px solid #e5e5e5;
    border-bottom: 1px solid #e5e5e5;

}
#container #right .right-body-table .right-table-title li:last-child,
#container #right .right-body-table .right-table-body li:last-child{
	border-right:1px solid #e5e5e5;
}

#container #right .right-body-table .right-table-body li{
	height: 58px;
    width: 155px;
    height: 60px;
    line-height: 60px;
    color: #696969;
    text-align: center;
    float: left;
    border-bottom: 1px solid #e5e5e5;
    border-left: 1px solid #e5e5e5;
}


/*弹窗- 新建测站信息*/
#createStation .modal-content{
    border-radius: 3px;
    width: 500px;
    margin-left: 50px;
    font-size: 14px;
}
#createStation .modal-header{
    padding: 9px 15px;
    border-bottom: 1px dashed #e5e5e5;
}
#createStation .modal-body{
    padding: 18px;
    color: #4e697c;
}
#createStation form>p{
    margin-bottom: 20px;
}
#createStation form>p>span{
    display: inline-block;
    width: 126px;
    text-align: right;
    padding-right: 16px;

}
#createStation form>p>input,
#createStation form>p .select-picker{
    width: 258px;
    height: 26px;
    border: 1px solid #e1e1e1;
    border-radius: 3px;
    text-indent: 10px;
    outline: none;
}
#createStation form>p .select-picker button{
    height: 24px;
    padding: 0;
    border: none;
}
#createStation form>p .select-picker button .filter-option{
    text-indent: 10px;
}
.bootstrap-select.btn-group .dropdown-menu li a span.text{
    display: block;
}
#createStation form>p>textarea{
    width: 257px;
    height: 79px;
    vertical-align: top;
    border: 1px solid #e1e1e1;
    border-radius: 3px;
    padding: 10px;
}
/*#createStation form>p .select-picker{
    width: 258px;
    height:
}*/
#createStation .saveAndQuit{
    text-align: center;
}
#createStation p.saveAndQuit>input{
    width: 64px;
    height: 24px;
    border-radius: 6px;
    text-align: center;
    text-indent: 0;
    font-size: 12px;
    margin: 0 3px;
}
#createStation .saveAndQuit>input:first-child{
    background-color: #4b90ed;
    color: white;
}
#createStation .saveAndQuit>input:last-child{
    background-color: #eee;
    color: #696969;
}
