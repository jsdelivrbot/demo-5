.btn {
    padding: 0;
    margin: 0;
}
.modal-dialog {
    margin: 0;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%)!important;
    z-index: 1;
}
/*弹窗- 增补-测报数据*/
 #augmentModal .modal-dialog {
    width: 502px;
}
#augmentModal .modal-header {
    // min-height: 16.43px;
    padding: 8px 15px 7px;
    // height: 40px;
    // line-height: 40px;
    color: #696969;
    font-size: 14px;
    border-bottom: 1px dashed #bfbfbf;
}
#augmentModal .modal-header .closeImg {
    outline: none;
}
#augmentModal .modal-body {
    padding: 0;
    margin: 20px 92px 20px 72px;
    font-size: 14px;
}
#augmentModal .modal-body-item {
    line-height: 26px;
    margin-bottom: 10px;
    color:#999;
}
#augmentModal .modal-body-item-name {
    width: 83px;
}
#augmentModal .collectionValue{
    padding-left: 4px;
    width: calc(100% - 87px);
    display:inline-block;
    vertical-align: top;
}
#augmentModal input {
    width: 248px;
    height: 26px;
    border: 1px solid #E1E1E1;
    border-radius: 3px;
    text-indent:10px;
}
#augmentModal .collectionValue .hintText{
    display: none;
    color: red;
    font-size: 12px;
    line-height: 16px;
}

#augmentModal .modal-footer {
    padding:0;
    padding-bottom: 20px;
    border-top: none;
    text-align: center;
}
#augmentModal .modal-footer .btn-keep,
#augmentModal .modal-footer .btn-delete{
    background-color: #4A91EA;
    border: none;
    color: #fff;
    width: 64px;
    height: 22px;
    line-height: 22px;
    text-align: center;
    border-radius: 5px;
    font-size: 12px;
}

#augmentModal .modal-footer .btn-keep:hover,
#augmentModal .modal-footer .btn-keep:active{
	outline:none;
	cursor:pointer;
	background:#4385d5;

}
#augmentModal .modal-footer .btn-cancel {
    width:64px;
    height: 22px;
    line-height: 21px;
    text-align: center;
    border-radius: 5px;
    font-size: 12px;
    background-color: #eeeeee;
    border: 1px solid #bfbfbf;
    margin-left:5px;
}
#augmentModal .timePicker{
    background-position: 226px center
}
/*弹窗- 自动测站信息*/
#createStation .modal-dialog{
    width: 1375px;
}
#createStation .modal-content{
    border-radius: 3px;
    margin: 0 auto;
    font-size: 14px;
}
#createStation .modal-header{
    padding: 8px 15px 7px;
    border-bottom: 1px dashed #bfbfbf;
}
#createStation .modal-body{
    padding: 20px;
    color: #4e697c;
}
#createStation .modal-body .mapBox{
    width:calc(100% - 460px);
