/*弹窗-区域平均降雨弹窗*/
.modal-backdrop.in{
    opacity: .3;
}
#averageRainfall .modal-content,
#averageRainfallModify .modal-content{
    border: 1px solid rgba(0,0,0,0);
    border-radius: 3px;
    color: #50697a;
    font-size: 14px;
    width: 800px;
    margin-left: 5px;
    margin-top: 88px;
    text-align: center;
    box-shadow: 0 0px 10px rgba(0,0,0,.3);
}
#averageRainfall .modal-header,
#averageRainfallModify .modal-header{
    padding: 8px 15px 7px;
    border-bottom: 1px dashed #bfbfbf;
}
#averageRainfall .modal-body{
    margin:20px;
    padding: 0;
    max-height: 510px;
    overflow: auto;
    border-top: 1px solid #e1e1e1;
    border-bottom: 1px solid #e1e1e1;
}
#averageRainfall .modal-body-content{
    border-left: 1px solid #e1e1e1;
    border-right: 1px solid #e1e1e1;
}
#averageRainfall .modal-body-content ul{
    border-bottom: 1px solid #e1e1e1;
}
#averageRainfall .modal-body-content ul:last-child{
    border-bottom: none;
}
#averageRainfall .modal-body-content ul>li{
    display: inline-block;
    width: 13%;
    height: 86px;
    margin: 0 8px;
    line-height: 86px;
    vertical-align: top;
}
#averageRainfall .modal-body-content div.verticalHalf{
    display: inline-block;
    width: 50%;
    line-height: 86px;
    vertical-align: top;
    text-align: left;
}
#averageRainfall .modal-body-content div.verticalHalf li{
    width: 24%;
    display: inline-block;
    vertical-align: middle;
    text-align: center;
}
#averageRainfall .modal-body-content div.verticalHalf li>span{
    display: block;
    line-height: normal;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
#averageRainfall .modal-body-content>ul>li span{
    display: inline-block;
    width: 100%;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

#averageRainfall .modal-body-content>ul>li.modify,
#averageRainfall .modal-body-content>ul>li.delete{
    width: 10%;
    color: #599bec;
    margin: 0 8px 0 30px;
}
#averageRainfall .modal-body-content>ul>li.modify div,
#averageRainfall .modal-body-content>ul>li.delete div{
    cursor: pointer;
    height: 20px;
    position: relative;
    top: 50%;
    transform: translateY(-50%);
}
#averageRainfall .modal-body-content>ul>li.delete{
    margin-right: 8px;
}
#averageRainfall .modal-body-content>ul>li.modify i,
#averageRainfall .modal-body-content>ul>li.delete i{
    display: inline-block;
    width: 20px;
    height: 20px;
    vertical-align: top;
    line-height: 20px;
    margin-right: 5px;
}
#averageRainfall .modal-body-content>ul>li.modify span,
#averageRainfall .modal-body-content>ul>li.delete span{
    display: inline-block;
    width: calc(100% - 25px);
    line-height: normal;
    vertical-align: top;
    text-align: left;
}
#averageRainfall .modal-body-content>ul>li.modify i{
    background: url(../imgs/dataManagement/dataManagement.png) no-repeat -5px -5px;
}

#averageRainfall .modal-body-content>ul>li.delete i{
    background: url(../imgs/dataManagement/dataManagement.png) no-repeat -35px -5px;
}



/*弹窗-区域平均降雨弹窗-修改*/
