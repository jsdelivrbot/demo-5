    line-height: 85px;
    cursor: default;
}
#right .rightContent3 .right-table-row-item:nth-child(1)>i{
    display: inline-block;
    width: 29px;
    height: 26px;
    background: url(../imgs/dataManagement/dataManagement.png) no-repeat -59px -1px;
    cursor: pointer;
