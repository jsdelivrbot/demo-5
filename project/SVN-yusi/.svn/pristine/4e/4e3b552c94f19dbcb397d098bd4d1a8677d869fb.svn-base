i.deletePic,i.editPic{
    display:inline-block;
    background:url('../imgs/dataManagement/dataManagement.png') no-repeat;
    cursor:pointer;
    vertical-align:middle;
}
i.deletePic{
    width:20px;
    height:20px;
    background-position:-35px -5px;
}
i.editPic{
    width: 20px;
    height: 20px;
    background-position:-5px -5px;    
}
#container {
    background: #F0F6F6;
    width:100%;
}
#left {
    width: 210px;
    height: calc(100% - 135px);
    background-color: #374050;
    float: left;
}
#left .left-item{
	height: 81px;
	line-height:81px ;
	color: #A9ACB1;
    text-align: center;
    font-size: 16px;
    cursor: pointer;
}
#left .left-item.active{
	color:#fff;
	background:#5D6371;
}

#right {
	padding:0px 110px 20px 90px;
  	margin-top:10px;
    width: calc(100% - 220px);
    height: calc(100% - 135px);
    float: right;
    background:#fff;

}
#right .right-title .create{
    width:100px;
    height:32px;
    line-height:32px;
    color: #fff;
    background-color: #4991EA;
    border-radius: 3px;
    border: none;
    font-size: 14px;
}
.pannel-content,.right-body-table,.right-title{
    max-width:1325.5px;
    font-size:14px;
}
/**第一页面**/
