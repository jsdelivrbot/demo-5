    display: table;
    text-align: center;
    line-height: normal;
}
#right .rightContent4 .right-table-row>li:first-child i{
    display: inline-block;
    width: 27px;
    height: 23px;
    vertical-align: middle;
    cursor: pointer;
    background: url(../imgs/dataManagement/dataManagement.png) no-repeat -59px -3px;
}
#right .rightContent4 div.modify,
#right .rightContent4 div.delete{
    display: inline-block;
    width: 50%;
    color: #599bec;
    margin: 0;
    cursor: pointer;
}
#right .rightContent4 div.modify i,
#right .rightContent4 div.delete i{
    display: inline-block;
    width: 20px;
    height: 20px;
    vertical-align: middle;
    cursor: pointer;
}
#right .rightContent4 div.modify i{
    background: url(../imgs/dataManagement/dataManagement.png) no-repeat -5px -5px;
}
#right .rightContent4 div.delete i{
    background: url(../imgs/dataManagement/dataManagement.png) no-repeat -35px -5px;
}


/**页脚**/
.notContent{padding: 15px 0;text-align: center;}
.pageChange {
    text-align: center;
    margin-top: 18px;
}
.pageChange .pageChange-span {
    border: 1px solid #bfbfbf;
    height: 25px;
    line-height: 25px;
    text-align: center;
    vertical-align: top;
}
.pageChange .pageChange-span2{
    width: 25px;
    cursor: default;
}

.pageChange .pageChange-span1, .pageChange .pageChange-span2 {
    margin-right: 17px;
}

.pageChange .pageChange-span1, .pageChange .pageChange-span3 {
    width: 60px;
    cursor: pointer;
}
/**按钮样式**/

.pageChange .pageChange-span-up,.pageChange .pageChange-span-up.active,
.pageChange .pageChange-span-down,.pageChange .pageChange-span-down.active{
    display: inline-block;
    width: 8px;
    height: 10px;
    background: url(../imgs/dataManagement/dataManagement.png) no-repeat ;
}
.pageChange .pageChange-span-up{
    margin-right:4px
}
.pageChange .pageChange-span-down{
    margin-left:4px
}
/**左侧按钮禁用**/
.pageChange .pageChange-span-up{
    background-position:-41px -35px;
}
/**左侧按钮激活**/
.pageChange-span1.active .pageChange-span-up{
    background-position: -23px -35px;
}

/**右侧按钮禁用**/
.pageChange .pageChange-span-down{
    background-position:-59px -35px;
}
/**右侧按钮激活**/
.pageChange-span3.active .pageChange-span-down{
    background-position: -5px -35px;
}
.pageChange-span1.disabled,.pageChange-span3.disabled{
    color:#d2d2d2;
    cursor:not-allowed;
}


.pageChange .txt {
    margin: 0 8px;
    cursor: default;
    vertical-align:sub;
}

/*若浏览器宽度小于1420px时*/
@media only screen and (max-width: 1420px) {
    #right{
        padding: 0px 60px 20px 40px;
    }
    /*第1页处理*/
    #right .rightContent1 .right-title .create,
    #right .rightContent1 .right-title .confirm{
        width: 75px;
    }
    #right .rightContent1 .timePicker{
        width: 160px;
        background-position: 136px center
    }
    #right .rightContent1 .bootstrap-select > .dropdown-toggle.bs-placeholder{
        width: 160px;
    }
    #right .rightContent1 .selectpickerBox {
        margin-right: 26px;
    }
    #right .rightContent1 .right-table-title-item,
    #right .rightContent1 .right-table-row-item {
        width: calc((100% - 340px) / 4);
    }
    #right .rightContent1 .right-table-title-item:nth-child(2),
    #right .rightContent1 .right-table-title-item:nth-child(4),
    #right .rightContent1 .right-table-row-item:nth-child(2),
    #right .rightContent1 .right-table-row-item:nth-child(4){
        width: 170px;
    }

}
