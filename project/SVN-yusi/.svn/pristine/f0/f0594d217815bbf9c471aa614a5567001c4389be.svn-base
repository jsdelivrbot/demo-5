}
#popInfo .line1>li:first-child{
	line-height: 60px;
}
#popInfo .line3{
	text-align: left;
	font-size: 0;
}
#popInfo .line3>ul{
	display: inline-block;
	vertical-align: top;
	float: left;
}
#popInfo .line3>ul:first-child{
