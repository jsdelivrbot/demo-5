}
#popInfo .line3 .item li{padding:0;}
#popInfo .line3 .city>li:first-child,
#popInfo .line3 .city>li:nth-child(2){
	width: 100%;

}
#popInfo .line3 .city li{
	width: 50%;
	float: left;
	height:21px;
	line-height:21px;
}
#popInfo .line3 li>span{
	display: inline-block;
	// width: 50%;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: clip;
	margin-top: -1px;
}

#popInfo .line3 li>span.item-data{
	height: 20px;
  border-left: 1px solid #ccc;
}
.tr2 .item-color{
