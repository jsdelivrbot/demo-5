/*右侧信息高度自适应*/
.mCSB_scrollTools{width:10px;right: 0;}
.tab-content{
	height:calc(100% - 49px);
}
/**panelRow2**/
.panelRow2{
    margin-top:12px;
    /*height: calc(100% - 320px);*/
	padding-bottom: 15px;
}

.tab-pannel1 .panelRow2{
	height:calc((100% - 40px) / 2);
}

.tab-pannel2 .panelRow2 {
	height:calc((100% - 40px) / 2);
}

//无数据占位
.tab-pannel1 .panelRow2 .table-responsive .noData{
	display:none;
}
.panelRow2-dataBind ul,.panelRow3-dataBind ul{cursor:pointer;width:98.2%;}
.panelRow2-dataBind ul:hover,.panelRow3-dataBind ul:hover{background:#e6e6e6;}
.panelRow2-dataBind ul.active,.panelRow3-dataBind ul.active{background:#e5e5e5;}

.tab-pannel1 .panelRow2 .table-responsive,
.tab-pannel2 .panelRow2 .table-responsive{
	height: calc(100% - 40px);
}

.panelRow2 .table-responsive>ul,
.panelRow3 .table-responsive>ul{
	background: #B7D2F2;
    padding-right: 8px;
}
.panelRow2 .table-responsive>ul>li,
.panelRow2 .panelRow2-dataBind ul>li,
.panelRow3 .table-responsive>ul>li,
.panelRow3 .panelRow3-dataBind ul>li{
	display: inline-block;
	width: 24%;
	text-align: center;
	padding: 8px 6px!important;
    line-height: 1.42857143;
    vertical-align: top;
}
.panelRow2 .panelRow2-dataBind ul>li:nth-child(2),
.panelRow3 .panelRow3-dataBind ul>li:nth-child(2){
    text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;
}

.panelRow2 .table-responsive>div{
	height: calc(100% - 36px);
}
.panelRow3 .table-responsive>div{
	height: 100%;
}
.panelRow3 .table-responsive .right-table-body{
	height: calc(100% - 49px);
}

.mCSB_inside > .mCSB_container{
	margin-right: 0px;
}

/**panelRow3**/
.panelRow3 {
    padding: 0 0 15px;
	height: calc((100% - 40px) / 2);
}
.tab-pannel1 .panelRow3{
	height: calc((100% - 40px) / 2);
}
.panelRow3 .table-responsive{
	height: calc(100% - 40px);
	overflow-y: hidden;
}

.panelRow3 .right-table-title {
    background-color: #B7D2F2;
}
.panelRow3 .right-table-title li {
    float: left;
    text-align: center;

    border-top: 1px solid #e5e5e5;
    border-left: 1px solid #e5e5e5;
    border-bottom: 1px solid #e5e5e5;
}
.panelRow3 .right-table-title-item{
    height:100%;
    /*line-height:50px;*/
    cursor: default;
}
.panelRow3 .right-table-title-item:nth-child(1),
.panelRow3 .right-table-title-item:nth-child(2){
	width:20%;
}
.panelRow3 .right-table-title-item:nth-child(3){
	width:60%;
}

.panelRow3 .right-table-title-item:nth-child(1),.panelRow3 .right-table-title-item:nth-child(2){
	line-height:47px;
}
.panelRow3 .right-table-title-item:nth-child(3)>span:nth-child(1) {
    width: 100%;
    display: block;
	height: 27px;
	line-height: 27px;
    border-bottom: 1px solid #e5e5e5;
}
.panelRow3  .right-table-title-item:nth-child(3)>span:nth-child(2) {
    border-right: 1px solid #e5e5e5;
}

.panelRow3  .right-table-title-item:nth-child(3)>span {
    display: inline-block;
    width: 50%;
    vertical-align: top;
}

.panelRow3 .right-table-row-item{

	height:100%;
    cursor: default;
}
.panelRow3 .right-table-row-item:nth-child(1),
.panelRow3 .right-table-row-item:nth-child(2){
	width:20%;

}

.panelRow3 .right-table-row-item:nth-child(3),
.panelRow3 .right-table-row-item:nth-child(4){
	width:30%;

}
.panelRow3 .right-table-title li:last-child, .panelRow3 .right-table-body li:last-child {
    border-right: 1px solid #e5e5e5;
}
.panelRow3 .right-table-body li {
    height: 30px;
    line-height: 30px;
    text-align: center;
    float: left;
    border-bottom: 1px solid #e5e5e5;
    border-left: 1px solid #e5e5e5;
}

.rainStatus .right-table-row-item{width:30%;}

.popInfo-content thead th:nth-child(1){
	width:80px;
}
.popInfo-content thead th:nth-child(2){
	text-align:left;
	padding:5px;
}

#popInfoTr td{vertical-align:top;}
#popInfoTr table{width:100%;border-collapse:collapse;}
#popInfoTr table  thead tr th{
	/*border:1px solid ;*/
	border-bottom: 1px solid #ccc;
}
#popInfoTr table tbody td{
	width:50%;
	border:1px solid ;
 	text-overflow: ellipsis;
    overflow: hidden;
    white-space: nowrap;

}

/**按钮样式**/
#water_search_btn:hover,
#water_search_btn:active,
#rain_search_btn:hover,
#rain_search_btn:active{
	cursor:pointer;
	background:#4385d5;
}

/*时段雨量详情弹窗*/
.bor-none{
	border: none !important;
}
.bor-top{
	border-top: 1px solid #ccc !important;
}
.bor-lef{
	border-left: 1px solid #ccc !important;
}
.bor-rig{
	border-right: 1px solid #ccc !important;
}
.bor-bot{
	border-bottom: 1px solid #ccc !important;
}
.bor-all{
	border: 1px solid #ccc !important;
}





