#map{
	width:100%;
	height:calc(100vh - 135px);


}
.mapBox{
	position:relative;
}
.contentRight {
    width: 496px;
    position: absolute;
    margin-top:10px;
    top:135px;
    right: 0px;
    background: #fff;
    min-height: 200px;
    height:calc(100vh - 145px);
    border-radius: 3px;
    box-shadow: 1px 3px 2px 2px rgba(0, 0, 0, 0.2);
    transition: all 0.2s ease 0s;
}

.contentRighthomei {
    cursor: pointer;
    width: 40px;
     height: 42px;
    text-align: center;
    border-right: 1px solid #312e28;
    float: right;
    padding-top: 15px;

}
.contentRightTabs {
	background:#616266;
    padding-left: 12px;
    height: 49px;

}
.contentRightTabs .tabs{
	display: inline-block;
	height: 43px;
    line-height: 43px;
    width: 25%;
    margin-top:6px;
    text-align: center;
    font-size: 18px;
    color:#fff;
    cursor:pointer;

}
.contentRightTabs .tabs.active{
	background:#fff;
	color:#000;
}
.tab-pannel{
	font-size:16px;
    padding:12px;
	display:none;
}
.tab-pannel .pannel-title{
    width:100%;
    height:40px;
    line-height:40px;
    background:#E6E6E6;
    padding:0 10px 0 14px;
}
.tab-pannel .pannel-title .pannel-titleText{
    font-size:16px;
    font-weight: bold;
}
.tab-pannel.active{
	display:block;

}
.pannel-content{

    margin-top:18px;
    padding:0px 6px 0px 22px;
}
.pannel-content .mar8{
    margin:0 8px;
}
.pannel-content .timePickerCaret{
    position:absolute;
    right:10px;
    top:17px;
    width:14px;
    height:12px;
    background:url(../imgs/common/common.png) no-repeat -5px -117px;
}
.detail{
    font-size:14px;
    color:#4A91EA;
    cursor:pointer;
}
.pickerBox{
    display:inline-block;
    position:relative;
}
.timePicker{
	width:170px;
    height: 38px;
    border: 1px solid #e5e5e5;
    text-indent: 5px;
    border-radius: 3px;
    outline: none;
    cursor:pointer;

}
.confirm{
    width:58px;
    height:38px;
    line-height:38px;
    margin-left:9px;
    border:none;
    border-radius: 3px;
    text-align:center;
    color:#fff;
    background:#4A91EA;
    cursor:pointer;
}
.panelRow2{
    margin-top:20px;
}
 .table{
    width:100%;
    text-align:center;

}
.table>thead {
    background:#B7D2F2;
}
.table>tbody{
    background:#F0F0F0;
}

.table thead tr th{
     width:25%;
     vertical-align:middle;
}

.table>thead>tr>th,.table>tbody>tr>td, .table>tbody>tr>th, .table>tfoot>tr>td, .table>tfoot>tr>th, .table>thead>tr>td, .table>thead>tr>th{
    border:none;

}
/**图例**/
.lengendBox{
	width:120px;
	// height:360px;
	position:absolute;
	right:522px;
	top:29px;
	background: rgba(255,255,255,.7);
	color:#000;
	box-shadow: 1px 1px 5px 1px rgba(0, 0, 0, 0.2);

}
.lengendBox .legendTitle{
	width:120px;
	height:50px;
	line-height:50px;
	text-align:center;
	font-size:16px;
	font-weight:bold;
	cursor:pointer;
}
.lengendBox .legendTitle .triangular{
	display:inline-block;
	width:12px;
	height:12px;
	background:url(../imgs/common/common.png) no-repeat -35px -87px;
}
.lengendBox .legendTitle .triangular.rotate{
	transform:rotate(180deg);
	-ms-transform:rotate(180deg); 	/* IE 9 */
	-moz-transform:rotate(180deg); 	/* Firefox */
	-webkit-transform:rotate(180deg); /* Safari 和 Chrome */
	-o-transform:rotate(180deg); 	/* Opera */
}
.legend-content{
	width:120px;
	padding-left:25px;
	color:#000;
	font-size:14px;


}

.legend-content li{
	height:38px;
	line-height:38px;
}
.legend-content li .legendCircle{
	display:inline-block;
	margin-right:5px;
	width:12px;
	height:12px;
	background:url(../imgs/common/common.png) no-repeat;
}
/**雨情**/
.legend-content li .legendCircle.gray{background-position: -81px -117px;}
.legend-content li .legendCircle.white{background-position:-103px -117px;}
.legend-content li .legendCircle.cyan{background-position:-125px -117px;}
.legend-content li .legendCircle.atrovirens{background-position:-5px -139px;}
.legend-content li .legendCircle.wathet{background-position: -69px -5px;}
.legend-content li .legendCircle.darkBlue{background-position:-91px -5px}
.legend-content li .legendCircle.croci{background-position: -113px -5px;}
.legend-content li .legendCircle.red{background-position:-135px -5px}
/**水情**/
.legend-content li .legendCircle.library{width:14px;height:14px;background-position:-6px -66px;}
.legend-content li .legendCircle.runway{width:14px;height:14px;background-position:-24px -66px;}

#rainLengend ,#rainLengend .legendTitle,#rainLengend .legend-content{
	width:210px;
}

#rainLengend .legend-content ul{
	display:inline-block;
	width:50%;
}
/*站名框*/
.popup{
	background: rgba(0,0,0,.6);
	color: white;
	position: relative;
	padding: 6px 11px;
	border-radius: 2px;
}
.popup:after{
	content: " ";
	display: inline-block;
	position: absolute;
	left: 50%;
	top: 100%;
	border: solid transparent;
	height: 0;
	width: 0;
	pointer-events: none;
	border-top-color: rgba(0,0,0,.6);
	border-width: 10px;
	margin-left: -10px;
}
.ol-overlay-container{
	transform: translate(-50%,-50%);
}

/*弹窗*/
.modal-backdrop{
	z-index: 0;
}
.modal-backdrop.in{
	opacity: .4
}

/*时段雨量详情*/
#popInfo .modal-content{
	width: 1024px;
	margin-left: -212px;
	margin-top: 230px;
	border-color: #616266 !important;
}
#popInfo .modal-header{
	background-color: #616266;
	color: #fff;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	text-align: center;
	font-size: 18px;
}
#popInfo .close{
	opacity: 1;
	color: white;
}
#popInfo .modal-body{
	/*height: 511px;*/
	padding: 0 30px 30px;
	text-align: center;
}
#popInfo .modal-body .popInfo-title{
	height: 70px;
	line-height: 70px;
	text-align: left;
}
#popInfo .modal-body .popInfo-title>span:nth-child(1){
	font-size: 16px;
}
#popInfo .modal-body .popInfo-content{
	border: 1px solid #ccc;
	border-right: none;
	height: 313px;
}
/*ul>li 形式*/
#popInfo ul{
	font-size: 0px;
}
#popInfo ul li{
	display: inline-block;
	vertical-align: top;
	border-bottom: 1px solid #ccc;
	border-right: 1px solid #ccc;
	box-sizing: border-box;
	font-size: 14px;
	width: 100%;
	height: 21px;
	line-height: 21px;
}
#popInfo ul:not(.item) li{
	padding: 0 8px;
}
#popInfo .line li:first-child{
	width: 80px;
	text-align: left;
}
#popInfo .line li:nth-child(2){
	width: calc(100% - 80px);
}
#popInfo .line1 li:nth-child(2)>span{
	position: absolute;
  top: 0;
  right: 0;
  bottom: 0;
  text-align: left;
  width: 100%;
  margin: auto;
  display: table;
	padding: 0 4px;
}
#popInfo .line1{
	height: 60px;
	text-align: left;
}
#popInfo .line1>li{
	height: 100%;
	width: 100%;
	position: relative;
	height: 100%;
}
#popInfo .line2 li.legend{
	text-align: left;
}
#popInfo .line2 li.legend span{
	display: inline-block;
	width: 24px;
	height: 14px;
	margin-right: 21px;
}
#popInfo .line2 li.legend span:nth-child(1){
	background-color: #e13131;
}
#popInfo .line2 li.legend span:nth-child(2){
	background-color: #e08827;
}
#popInfo .line2 li.legend span:nth-child(3){
	background-color: #f0ef07;
}
#popInfo .line2 li.legend span:nth-child(4){
	background-color: #55ccf4;
}
#popInfo .line2 li.legend span:nth-child(5){
	background-color: #87d446;
}
#popInfo .line1>li:first-child{
	line-height: 60px;
}
#popInfo .line3{
	text-align: left;
	font-size: 0;
}
#popInfo .line3>ul{
	display: inline-block;
	vertical-align: top;
	float: left;
}
#popInfo .line3>ul:first-child{
	width: 80px;
}
#popInfo .line3>ul:first-child>li{
	width: 100%;
}
#popInfo .line3>ul:first-child>li:nth-child(3){
	height: 189px;
	line-height: 189px;
}
#popInfo .line3 .item{
	width: 110px;
	text-align: center;
}
#popInfo .line3 .item>li:nth-child(1){
	color: #4791ea;
	font-weight: bold;
}
#popInfo .line3 .city{
	width: 221px;
}
#popInfo .line3 .city>li:first-child,
#popInfo .line3 .city>li:nth-child(2){
	width: 100%
}
#popInfo .line3 .city li{
	width: 50%;
	float: left;
}
#popInfo .line3 li>span{
	display: inline-block;
	width: 50%;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: clip;
	margin-top: -1px;
}
#popInfo .line3 li>span:nth-child(1){
	text-align: left;
	padding: 0 3px;
}
#popInfo .line3 li>span.item-data{
	height: 20px;
  border-left: 1px solid #ccc;
}
#popInfo .line3 .item-color1{
	background-color: #e13131;
}
#popInfo .line3 .item-color2{
	background-color: #e08827;
}
#popInfo .line3 .item-color3{
	background-color: #f0ef07;
}
#popInfo .line3 .item-color4{
	background-color: #55ccf4;
}
#popInfo .line3 .item-color5{
	background-color: #87d446;
}
#popInfo .line3 li:last-child,
#popInfo .city li:nth-last-child(2){
	border-bottom: none;
}

