#map{
	width:100%;
	height:calc(100vh - 135px);

}
.contentRight {
    width: 496px;
    position: absolute;
    margin-top:10px;
    top:135px;
    right: -7px;
    background: #fff;
    min-height: 200px;
    height:calc(100vh - 145px);
    border-radius: 3px;
    box-shadow: 1px 3px 2px 2px rgba(0, 0, 0, 0.2);
    transition: all 0.2s ease 0s;
}

.contentRighthomei {
    cursor: pointer;
    width: 40px;
     height: 42px;
    text-align: center;
    border-right: 1px solid #312e28;
    float: right;
    padding-top: 15px;

}
.contentRightTabs {
	background:#616266;
    padding-left: 10px;
    height: 49px;

}
.contentRightTabs .tabs{
	display: inline-block;
	height: 43px;
    line-height: 43px;
    width: 25%;
    margin-top:6px;
    text-align: center;
    font-size: 18px;
    color:#fff;
    cursor:pointer;

}
.contentRightTabs .tabs.active{
	background:#fff;
	color:#000;
}
.tab-pannel{
    padding:12px 16px;
	display:none;
}
.tab-pannel .pannel-title{
    width:100%;
    height:40px;
    line-height:40px;
    background:#E6E6E6;
    padding:0 10px 0 14px;
    margin-bottom:18px;
}
.tab-pannel .pannel-title .pannel-titleText{
    font-size:16px;
    font-weight: bold;
}
.tab-pannel.active{
	display:block;

}
.pannel-content{
    padding:0px 6px 0px 22px;
}
.pannel-content .mar10{
    margin:0 10px;
}
.pannel-content .caret{
    position:absolute;
    right:10px;
    top:17px;
}
.pickerBox{
    display:inline-block;
    position:relative;
}
.timePicker{
    height: 38px;
    border: 1px solid #e5e5e5;
    text-indent: 5px;
    border-radius: 3px;
    outline: none;
    cursor:pointer;

}
.confirm{
    width:58px;
    height:38px;
    line-height:38px;
    margin-left:8px;
    border:none;
    border-radius: 3px;
    text-align:center;
    color:#fff;
    background:#4A91EA;
    cursor:pointer;
}

/*弹窗*/
.modal-backdrop{
	z-index: 0;
}
.modal-backdrop.in{
	opacity: .4
}

/*时段雨量详情*/
#popInfo .modal-content{
	width: 1024px;
	margin-left: -212px;
	margin-top: 230px;
	border-color: #616266 !important;
}
#popInfo .modal-header{
	background-color: #616266;
	color: #fff;
	border-top-left-radius: 4px;
	border-top-right-radius: 4px;
	text-align: center;
	font-size: 18px;
}
#popInfo .close{
	opacity: 1;
	color: white;
}
#popInfo .modal-body{
	height: 511px;
	padding: 0 30px;
	text-align: center;
}
#popInfo .modal-body .popInfo-title{
	height: 70px;
	line-height: 70px;
}
#popInfo .modal-body .popInfo-content{
	border: 1px solid gray;
	height: 407px;
}
#popInfo table td,#popInfo table th{
	border: 1px solid gray;
}
