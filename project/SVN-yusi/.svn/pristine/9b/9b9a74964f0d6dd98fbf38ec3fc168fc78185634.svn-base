              <div class="table-responsive">
                <table class="table">
                  <thead>
                    <tr>
                       <th>编号</th>
                       <th>站名</th>
                       <th>水位(m)</th>
                       <th>库容(10<sup>6</sup>m<sup>2</sup>)</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr>
                      <td>17456890</td>
                      <td>北洋坝</td>
                      <td>14.00</td>
                      <td>300.00</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>

            <!--河道信息-->
            <div class="panelRow3">
              <div class="pannel-title"><span class="pannel-titleText">河道信息</span></div>
              <div class="table-responsive">
