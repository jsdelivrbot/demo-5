#container {
    background: #343D4E;
    width: 100%;
    min-width:1200px;
    position:absolute;
    top:135px;
    left:0;
    bottom:0;
}
#left {
    width: 186px;
    background-color: #374050;
    position:absolute;
    height:auto;
}
#left .left-item {
    height: 81px;
    line-height: 81px;
    color: #A9ACB1;
    padding-left:40px;
    font-size: 16px;
    cursor: pointer;
}
#left .left-item.active {
    color: #fff;
    background: #5D6371;
}
#right {
	margin-left:185px;
    padding:10px;
    background:#EFF6F6;
    width: calc(100% - 185px);
    // position:absolute;
    // left:0;
    // right:0;
    // top:0;
    // bottom:0;
    height:100%;

}

#right .right-title{
    background:#fff;
    padding:30px 40px;
    margin-bottom:10px;
}
#right .right-title{
    text-align:center;
}
#right .right-title .timepickerBox{
    margin-top:4px;
}
.timePicker {
    width: 180px;
    height: 32px;
    border: 1px solid #e5e5e5;
    text-indent: 10px;
    border-radius: 3px;
    outline: none;
    cursor: pointer;
}
.timePicker {
    background: url(../imgs/common/Real-time-3.png) 154px center no-repeat;
}
#right  .right-title .btn-tabs button {
    padding:9px 15px;
    color: #fff;
    background-color: #fff;
    border-radius: 3px;
    border: 1px solid #4991EA;
    color: #4991EA;
    font-size:14px;
}
#right .right-title .btn-tabs button.active {
    background-color: #4991EA;
    color: #fff;
}
#right .right-title .export {
    padding:9px 46px;
    color: #fff;
    background-color: #5CB85C;
    border-radius: 3px;
    border: none;
    font-size: 14px;
}
.selectBox {
    display:inline-block;
    position:relative;
}
.confirm {
    width: 100px;
    height: 32px;
    line-height: 32px;
    margin-left: 15px;
    border: none;
    border-radius: 3px;
    text-align: center;
    color: #fff;
    background: #5cb85c;
    cursor: pointer;
}

.prevButton, .nextButton {
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    display: inline-block;
    vertical-align: middle;
    width: 10px;
    height: 10px;
    background: url(../imgs/common/common.png) no-repeat;
    /*margin: 0 0 4px 0;*/
    background-clip: content-box;
    box-sizing: content-box;
    padding: 15px 15px;
}
.prevButton {
    background-position: -98px -126px;
    margin-right: 10px;
}
.nextButton {
    background-position: -119px -126px;
}
.prevButton.active {
    background-position: -64px -126px;
    cursor: pointer;
}
.nextButton.active {
    background-position: -81px -126px;
    cursor: pointer;
}
.selectView{
    display:inline-block;
    vertical-align: middle;
    width: 121px;
    height: 38px;
    line-height:38px;
    text-align: center;
    text-indent: 5px;
    font-size: 16px;
    color: #4991EA;
    cursor:pointer;

}
.selectBox .dropDonMenu{
    display:none;
    position: absolute;
    top:40px;
    left:28px;
    width:130px;
    height: 210px;
    background:#fff;
    box-shadow: 1px 1px 1px #ddd;
    z-index: 1;
}
.selectBox .dropDonMenu li{
    line-height:40px;
}
.selectBox .dropDonMenu li:hover{
    cursor:pointer;
}
.selectBox .dropDonMenu li:hover,
.selectBox .dropDonMenu li:active{
    background:#4991EA;
    color:#fff;
}
.rightContent{
    width: 100%;
    height:calc(100% - 100px);
}
.rightContent .echart{
    margin-right:5px;
}
.rightContent .echart,.rightContent .echartContent{
    /*width:986px;*/
    height:calc(100% - 62px);
    background-color:#fff;
}
.panel{
    margin-bottom:0;
}
.panel-body{
    padding:0;
}
.rightContent .echart{
    // width: 59.3%;
    height:100%;
}
.rightContent .rightTable{
    margin-left:5px;
    height:100%;
    background-color:#fff;
}
.rightContent .rightTable .panel-body,.rightContent .rightTable .panel-body .table-responsive{
    height:100%;
    overflow-y:hidden;
}
 /*table tbody tr{line-height:50px;}
 table tbody tr td{text-align:center;border-top:none!important;line-height:60px!important;}
 .table>thead>tr>th{border-bottom:none!important;}
 .rightContent .rightTable tbody{
     max-height: 200px; overflow:auto;
 }*/
.table-responsive ul{
    width:100%;
}
.table-responsive>ul.header{
    border-bottom:1px solid #E1E1E1;
    height: 62px;
    line-height: 62px;
}
.table-responsive ul li{
    display:inline-block;
    text-align:center;
    width:33%;
}
.table-responsive .data-bind li{
    line-height:60px;
}
.table-responsive .scollBox{
   height:calc(100% - 72px);
}
.mCSB_inside > .mCSB_container{
    margin-right:0;
}
.rightContentNoData{
    width:100%;
    height:calc(100% - 100px);
    background:#fff;
    position:relative;
}
/*无数据占位*/
.takePlace{
    position: absolute;
    left: 50%;
    top: 50%;
    transform:translate(-50%,-50%);

}
.takePlace p{
    margin-top:10px;
}

.echartTitle{
    height:62px;
    line-height:62px;
    padding-left:50px;
    border-bottom:1px solid #E1E1E1;
    background:url('../imgs/common/line-chart.png') no-repeat 22px center;
}
.echartTitle .ml108{
    margin-left:108px;
}
@media only screen and (min-width: 992px){
    .echart{
        width: 60%;
    }
    .rightTable{
        width: calc(100% - 61%);
    }
}

