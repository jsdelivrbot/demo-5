        </div>
    </div>
    <!-- 内容 -->
    <div id="wrap">
        <div class="wrap-title clearfix">
            <div class="wrap-title-first">
                <button type="button" name="button" class="active" id="days">日统计</button>
                <button type="button" name="button" id="months">月统计</button>
                <button type="button" name="button" id="years">年统计</button>
            </div>
            <div class="wrap-title-second hide">
                <button type="button" name="button">人工站</button>
                <button type="button" name="button" class="active">自动站</button>
            </div>
            <div class="wrap-title-third">
                <!--日期控件-->
                <div class="pickerBox">
                    <i class="prevButton mr20"></i>
                    <input type="text" id="daysPicker" class="timePicker timePicker1" readonly="readonly">
                    <input type="text" id="monthsPicker" class="timePicker timePicker2 hide" readonly="readonly">
                    <input type="text" id="yearsPicker" class="timePicker timePicker3 hide" readonly="readonly">
                    <i class="nextButton ml20"></i>
                </div>
            </div>
            <div class="wrap-title-forth">
                <button type="button" name="button" data-table="#rainfallTable">导出</button>
            </div>
        </div>
        <div class="wrap-content">
            <table id="rainfallTable"></table>
        </div>
    </div>


