package mybatisPro.mybatisService.impl;

import java.util.List;
import java.util.Map;

/**
 * 雨量统计接口
 * 
 * @date 2017年8月14日
 * @version 1.0
 */
public interface RainfallStatisticsService {

	/**
	 * 雨量年统计
	 * 
	 * @param year
	 * @param type
	 * @return
	 */
	public List<Map<String, Object>> statisticsByYear(String year, Integer type);

	/**
	 * 雨量天统计
	 * 
	 * @param days
	 * @param type
	 * @return
	 */
	public List<Map<String, Object>> statisticsByDay(String days);

	/**
	 * 雨量月统计
	 * 
	 * @param month
	 * @param type
	 * @return
	 */
	public List<Map<String, Object>> statisticsByMonth(String month, Integer type);

}
