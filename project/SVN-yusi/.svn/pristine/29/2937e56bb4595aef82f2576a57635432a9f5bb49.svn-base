        <div class="contentRightTabs">
          <div class="tabs tabs1 active">实时水情</div>
          <div class="tabs tabs2">实时雨情</div>
        </div>
        <div class="tab-content">
          <div class="tab-pannel tab-pannel1 active">
            <div class="panelRow1">
              <div class="pannel-title"><span class="pannel-titleText">条件选择</span></div>
              <div class="pannel-content">
                <div class="pickerBox"><input type="text" id="starttime" class="timePicker" readonly="readonly" placeholder="开始时间"/><span class="caret timepickerCaret"></span></div><span class="mar10">到</span><div class="pickerBox"><input type="text" id="endtime" class="timePicker" readonly="readonly" placeholder="结束时间"/><span class="caret timepickerCaret"></span></div>

                 <button type="button" id="grid_search_btn" class="confirm">确定</button>
              </div>
            </div> 

            <div class="panelRow2">
              <div class="pannel-title"><span class="pannel-titleText">水库信息</span></div>
              <div class="table-responsive">
                <table class="table">
                  <thead>
                    <tr>
                       <th>编号</th>
                       <th>站名</th>
                       <th>水位(m)</th>
                       <th>库容(10<sup>6</sup>m<sup>2</sup>)</th>                       
                    </tr>    
                  </thead>
                  <tbody>
                    <tr>
                      <td>17456890</td>
                      <td>北洋坝</td>
                      <td>14.00</td>
                      <td>300.00</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>  


            <div class="panelRow3">
              <div class="pannel-title"><span class="pannel-titleText">河道信息</span></div>
              <div class="table-responsive">
                <table class="table">
                  <thead>
                    <tr>
                       <th>编号</th>
                       <th>站名</th>
                       <th>水位(m)</th>
                       <th>流量(m<sup>3</sup>/s)</th>                       
                    </tr>    
                  </thead>
                  <tbody>
                    <tr>
                      <td>17456890</td>
                      <td>北洋坝</td>
                      <td>14.00</td>
                      <td>3000.00</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>                         

          </div>
          <div class="tab-pannel tab-pannel2">
            <div class="panelRow1">
              <div class="pannel-title"><span class="pannel-titleText">条件选择</span></div>
              <div class="pannel-content">
                <div class="pickerBox"><input type="text" id="starttime1" class="timePicker" readonly="readonly" placeholder="开始时间"/><span class="caret"></span></div><span class="mar10">到</span><div class="pickerBox"><input type="text" id="endtime1" class="timePicker" readonly="readonly" placeholder="结束时间"/><span class="caret"></span></div>

                 <button type="button" id="grid_search_btn" class="confirm">确定</button>
              </div>
            </div>

            <div class="panelRow2">
              <div class="pannel-title"><span class="pannel-titleText">雨量信息</span><span class="fr detail">详情</span></div>
              <div class="table-responsive">
                <table class="table">
                  <thead>
                    <tr>
                       <th>编号</th>
                       <th>站名</th>
                       <th>雨量</th>
                       <th>站址</th>                       
                    </tr>    
