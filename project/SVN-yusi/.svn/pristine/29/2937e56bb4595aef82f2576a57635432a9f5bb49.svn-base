                  <tbody>
                    <tr>
                      <td>1</td>
                      <td>北洋坝</td>
                      <td>14.0</td>
                      <td>泽雅镇林坝村</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>

          <div class="panelRow3">
              <div class="pannel-title"><span class="pannel-titleText">雨量统计</span><span class="fr detail">详情</span></div>
              <div class="table-responsive">
                <table class="table">
                  <thead>
                    <tr>
                      <th rowspan="2">区域</th>
                      <th rowspan="2">平均雨量</th>
                      <th colspan="2">雨量情况</th>
                    </tr>
                    <tr>
                      <th >站名</th>
                      <th >雨量</th>
                    </tr>    
                  </thead>
                  <tbody>
                    <tr>
                      <td>大兴区</td>
                      <td>14.0</td>
                      <td>北冰洋</td>
                      <td>18.0</td>
                    </tr>
                  </tbody>
                </table>
              </div>
            </div>
          </div>
        </div>
