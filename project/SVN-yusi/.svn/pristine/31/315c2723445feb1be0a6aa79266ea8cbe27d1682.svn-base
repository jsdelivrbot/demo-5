.nowrap{
	overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}

/**清除连续点击时间控件左右按钮蓝色背景**/
.pickerBox{
-webkit-user-select:none;
-moz-user-select:none;
-ms-user-select:none;
user-select:none;
}

/*修正jqGrid的样式*/
.ui-jqgrid .ui-jqgrid-bdiv{
    width: 100% !important;
}
.table-responsive{
    /*overflow-x: hidden;*/
}
.ui-jqgrid-bdiv>div{
    overflow: hidden !important;
    width: calc(100% - 1px);
}
.ui-jqgrid .ui-jqgrid-htable .ui-th-div{
    height: 24px;
}
.ui-jqgrid{
    /*border-right: none;*/
    border-bottom: none;
}
.ui-jqgrid .ui-jqgrid-pager, .ui-jqgrid .ui-jqgrid-toppager{
	border:none;
}
table{
    font-size: 14px;
}
.table>thead>tr>th,.table>tbody>tr>td{
    padding:8px 0;
}
.ui-jqgrid .ui-jqgrid-htable thead th,.ui-jqgrid .ui-jqgrid-btable tbody tr.jqgrow td{
    padding-right: 0;
}


.ui-jqgrid-btable tr.jqgrow td:nth-child(2){
	overflow: hidden;
	white-space: nowrap;
	text-overflow : ellipsis;
}
.wrap-content table>thead>tr{
    background-color: #f2f2f2;
}
table>thead>tr .ui-th-div{
    font-weight: bold;
}

/**无数据-样式**/
.frozen{
	color: lightgray;
}
/*errorBtn弹窗*/
#errorInfo{
	font-size: 14px;
}

/*去掉IE中input的x*/
input::-ms-clear,
input::-ms-reveal{
    display: none !important;
}

.table>tbody>tr.success>td, .table>tbody>tr.success>th, .table>tbody>tr>td.success, .table>tbody>tr>th.success, .table>tfoot>tr.success>td, .table>tfoot>tr.success>th, .table>tfoot>tr>td.success, .table>tfoot>tr>th.success, .table>thead>tr.success>td, .table>thead>tr.success>th, .table>thead>tr>td.success, .table>thead>tr>th.success{
	background:#e5e5e5;
}
.table>tbody>tr.active,.table>thead>tr.active{
	background:#e3f1fa!important;
}

.modal{
	overflow: hidden !important;
}

@media screen and (max-width: 767px){
	.table-responsive{
		overflow-x: hidden;
	}
}
