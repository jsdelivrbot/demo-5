.table-responsive ul{
    width:100%;
}
.table-responsive ul li{
    display:inline-block;
    text-align:center;
    width:33%;
}
.table-responsive .data-bind li{
    line-height:60px;
}
.table-responsive .scollBox{
    max-height: 500px;
}
.mCSB_inside > .mCSB_container{
    margin-right:0;
}



@media only screen and (max-width:1400px){
    #left{
        width: 130px;
    }
    #right{
        margin-left: 130px;
        width: calc(100% - 130px);
    }
    #right .right-title{
        padding: 30px 20px;
    }
    .timePicker{
        font-size: 14px;
        text-indent: 5px;
        width: 154px;
        background-position: 132px center;
    }
    #right  .right-title .btn-tabs button{
        padding: 9px;
    }
    .mr20{
        margin-right: 6px;
    }
    #right .right-title .export{
        padding: 9px 10px;
    }
}

