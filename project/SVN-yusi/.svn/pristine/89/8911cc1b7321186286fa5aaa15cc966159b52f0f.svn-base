#container {
    background: #343D4E;
    width: 100%;
    position:absolute;
    top:135px;
    left:0;
    bottom:0;
}
#left {
    width: 185px;
    background-color: #374050;
    position:absolute;
    height:auto;
}
#left .left-item {
    height: 81px;
    line-height: 81px;
    color: #A9ACB1;
    padding-left:40px;
    font-size: 16px;
    cursor: pointer;
}
#left .left-item.active {
    color: #fff;
    background: #5D6371;
}
#right {
	margin-left:185px;
    padding:20px;
    background:#EFF6F6;
    width: calc(100% - 185px);
    position:absolute;
    left:0;
    right:0;
    top:0;
    bottom:0;
    height:100%;

}

#right .right-title{
    background:#fff;
    padding:30px 40px;
    margin-bottom:20px;
}
#right .right-title{
    text-align:center; 
}
#right .right-title .timepickerBox{
    margin-top:4px;
}
.timePicker {
    width: 180px;
    height: 32px;
    border: 1px solid #e5e5e5;
    text-indent: 10px;
    border-radius: 3px;
    outline: none;
    cursor: pointer;
}
.timePicker {
    background: url(../imgs/common/Real-time-3.png) 154px center no-repeat;
}
#right  .right-title .btn-tabs button {
    padding:9px 15px;
    color: #fff;
    background-color: #fff;
    border-radius: 3px;
    border: 1px solid #4991EA;
    color: #4991EA;
    font-size:14px;
}
#right .right-title .btn-tabs button.active {
    background-color: #4991EA;
    color: #fff;
}
#right .right-title .export {
    padding:9px 46px;
    color: #fff;
    background-color: #5CB85C;
    border-radius: 3px;
    border: none;
    font-size: 14px;
}
.selectBox {
    display:inline-block;
    position:relative;
}
.confirm {
    width: 100px;
    height: 32px;
    line-height: 32px;
    margin-left: 15px;
    border: none;
    border-radius: 3px;
    text-align: center;
    color: #fff;
    background: #5cb85c;
    cursor: pointer;
}

.prevButton, .nextButton {
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    display: inline-block;
    width: 8px;
    height: 10px;
    background: url(../imgs/common/common.png) no-repeat;
}
.prevButton {
    background-position: -113px -141px;
}
.nextButton {
    background-position: -133px -141px;
}
.prevButton.active {
    background-position: -80px -141px;
    cursor: pointer;
}
.nextButton.active {
    background-position: -95px -141px;
    cursor: pointer;
}
.selectView{
    display:inline-block;
    vertical-align: middle;
    width: 121px;
    height: 38px;
    line-height:38px;
    text-align: center;
    text-indent: 5px;
    font-size: 16px;
    color: #4991EA;
    cursor:pointer;

}
.selectBox .dropDonMenu{
    display:none;
    position: absolute;
    top:40px;
    left:28px;
    width:130px;
    height: 210px;
    background:#fff;
    box-shadow: 1px 1px 1px #ddd;
    z-index: 1;
}
.selectBox .dropDonMenu li{
    line-height:40px;
}
.selectBox .dropDonMenu li:hover{
    cursor:pointer;
}
.selectBox .dropDonMenu li:hover,
.selectBox .dropDonMenu li:active{
    background:#4991EA;
    color:#fff;
}
.rightContent{
    width: 100%;
    height:calc(100% - 120px);
}
.rightContent .echart{
    margin-right:2%;
}
.rightContent .echart,.rightContent .echartContent{
    /*width:986px;*/
    height:100%;
    background-color:#fff;
}
.rightContent .echart{
    width: 58%;
    height:100%;
    padding:0.5%;
}
.rightContent .rightTable{
    width: 40%;
    height:100%;
    background-color:#fff;
}
.rightContent .rightTable .panel-body{
    
}
