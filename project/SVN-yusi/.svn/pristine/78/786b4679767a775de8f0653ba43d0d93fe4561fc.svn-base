#wrap {
    width: 100%;
    margin: 11px 0;
    padding: 0 78px;
    background-color: white;
}
.wrap-title {
    padding: 25px 0;
    text-align: center;
    position: relative;
}
/*.wrap-title>div {
    font-size: 0;
}*/

.wrap-title-third {
    display: inline-block;
}
.wrap-title-forth{
    position:absolute;
    top:36px;
    right:0;
}

.prevButton, .nextButton {
    display: inline-block;
    width: 8px;
    height: 10px;
    background: url(../imgs/common/common.png) no-repeat;
}

.prevButton {
    background-position: -113px -141px;
}
.nextButton {
    background-position: -133px -141px;
}
.prevButton.active {
    background-position: -80px -141px;
    cursor: pointer;
}
.nextButton.active {
    background-position: -95px -141px;
    cursor: pointer;
}
.timePicker {
    width: 121px;
    height: 38px;
    text-align: center;
    text-indent: 5px;
    font-size:16px;
    font-weight:bold;
    color:#4991EA;
    border:none;
    outline: none;
    cursor: pointer;
}


.circle{
	display:inline-block;
	border-radius: 50%;
	width:8px;
	height:8px;
}
.circle.red{
	background:#E7211B;
}
.circle.green{
	background:#01CF62;
}
.circle.yellow{
	background:#FEDD01;
}
.wrap-content{
    padding-bottom:30px;
}
