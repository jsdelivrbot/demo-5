#wrap {
    width: 100%;
    margin: 11px 0;
    padding: 0 78px;
    background-color: white;
}
.wrap-title {
    padding: 25px 0;
    text-align: center;
}
// .wrap-title>div {
//     font-size: 0;
// }

.wrap-title-third {
    display: inline-block;
}

.prevButton, .nextButton {
    display: inline-block;
    width: 8px;
    height: 10px;
    background: url(../imgs/common/common.png) no-repeat;
    cursor: pointer;
}
.prevButton {
    background-position: -80px -141px;
}
.nextButton {
    background-position: -95px -141px;
}

.circle{
	display:inline-block;
	border-radius: 50%;
	width:10px;
	height:10px;
}
.circle.red{
	background:#E7211B;
}
.circle.green{
	background:#01CF62;
}
.circle.yellow{
	background:#FEDD01;
}
