.container-top{background:#fff;margin-top:10px;padding-top:32px;box-shadow:1px 10px 15px #E5EFFB;}
.container-top p{margin-bottom:50px;}
.container-top .title{margin-left:28px;}
.container-top .circle{display:inline-block;width:12px;height:12px;line-height:12px;text-align:center;border-radius:50%;margin-right:8px;vertical-align:middle;}
.container-top .circle.red{background:#E7211E;}
.container-top .circle.green{background:#01CF60;}
.container-top .circle.yellow{background:#FFDE01;}
.container-top .circle.gray{background:#A4A4A4;}
.top-right-last{margin-right:42px;}
.container-top button{
	width:90px;
	height:32px;
	line-height:32px;
	text-align:center;color: #fff;
    background-color: #4991EA;
    border-radius: 3px;
    border: none;
    font-size: 14px;
    margin-left:28px;
}
.left,.right{display: inline-block;width:46px;vertical-align:middle;}
.left span,.right span{display: inline-block;width:44px;height:44px;cursor: pointer;border-radius: 50%;background:url('../imgs/common/common.png') no-repeat;}
.left span.leftBtn{background-position:-171px -56px;}
.left span.leftBtn.disabled{background-position:-171px -107px;}
.right span.rightBtn{background-position:-233px -56px;}
.right span.rightBtn.disabled{background-position:-233px -107px;}
.sliderBar{display:inline-block;vertical-align:middle;}
.sliderBar li{position: relative;float: left;width: 60px;height: 110px;text-align: center;cursor: pointer;}
.sliderBar li.active{background: #FCF8E3;cursor: pointer;background:#4791EA;color:#fff;}
.sliderBar li span{display:inline-block;width:46px;height:46px;line-height:46px;margin-top:30px;margin-bottom:12px;text-align:center;border-radius:50%;background:#04CE62;color:#fff;font-size: 14px;}
.sliderBar li.active span{background:#fff;color:#04CE62;}
