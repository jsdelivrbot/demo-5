#container {
    background: #F0F6F6;
    width:100%;
}
#container #left {
    width: 210px;
    height: calc(100% - 135px);
    background-color: #374050;
    float: left;
}
#container #left .left-item{
	height: 81px;
	line-height:81px ;
	color: #A9ACB1;
    text-align: center;
    font-size: 14px;
    cursor: pointer;
}
#container #left .left-item.active{
	color:#fff;
	background:#5D6371;
}

#container #right {
	padding:0px 73px 20px 56px;
  	margin-top:10px;
    width: calc(100% - 220px);
    height: calc(100% - 135px);
    float: right;
    background:#fff;
   
}
#container #right .right-title{
	height:82px;
	line-height:82px;
	text-align:center;
    font-size: 18px;
}

#container #right .right-title .create{
	width:120px;
	height:34px;
	line-height:34px;
    color: #fff;
    background-color: #4991EA;
    border-radius: 3px;
    border: none;
    margin-top:22px;
    margin-right: 10px;
    font-size: 16px;
}
#container #right .right-body-table{
    font-size: 14px;
}
#container #right .right-body-table .right-table-title{
    line-height: 48px;
    background:#F2F2F2;
}
#container #right .right-body-table .right-table-title li{
    float: left;
    /*width: 11.1%;*/
    text-align: center;
    color: #696969;
    border-top: 1px solid #e5e5e5;
    border-left: 1px solid #e5e5e5;
    border-bottom: 1px solid #e5e5e5;
}
#container #right .right-body-table .right-table-title li:last-child,
#container #right .right-body-table .right-table-body li:last-child{
	border-right:1px solid #e5e5e5;
}
#container #right .right-table-body{
    color: #50697a;
}
#container #right .right-body-table .right-table-body li{
    height: 60px;
    line-height: 60px;
    text-align: center;
    float: left;
    border-bottom: 1px solid #e5e5e5;
    border-left: 1px solid #e5e5e5;
}
#container #right .right-body-table .right-table-body li:nth-child(9) span{
    cursor:pointer;
}
#container #right .right-body-table .right-table-body li:nth-child(9) .editBox{
    line-height:normal;
    cursor:pointer;
}
#container #right .right-body-table .right-table-body li:nth-child(9) i.editPic{
    display:inline-block;
    width:20px;
    height:20px;
    background:url(../imgs/dataManagement/dataManagement.png) no-repeat -5px -5px;
    cursor:pointer;
    vertical-align:text-bottom;

}
#right .right-table-title-item:nth-child(1),
#right .right-table-row-item:nth-child(1){
    width: 15.8%;
}
#right .right-table-title-item:nth-child(2),
#right .right-table-row-item:nth-child(2){
    width: 12.5%;
}
#right .right-table-title-item:nth-child(3),
#right .right-table-row-item:nth-child(3){
    width: 8.5%;
}
#right .right-table-title-item:nth-child(4),
#right .right-table-row-item:nth-child(4){
    width: 8.5%;
}
#right .right-table-title-item:nth-child(5),
#right .right-table-row-item:nth-child(5){
    width: 8.5%;
}
#right .right-table-title-item:nth-child(6),
#right .right-table-row-item:nth-child(6){
    width: 18%;
}
#right .right-table-title-item:nth-child(7),
#right .right-table-row-item:nth-child(7){
    width: 8.5%;
}
#right .right-table-title-item:nth-child(8),
#right .right-table-row-item:nth-child(8){
    width: 8.5%;
}
#right .right-table-title-item:nth-child(9),
#right .right-table-row-item:nth-child(9){
    width: 11.2%;
    color: #4891ea;
}
#right .right-table-row-item:nth-child(6){
    position: relative;
    height: 100%;
}
#right .right-table-row-item:nth-child(6) span{
    position: absolute;
   top: 0;
   right: 0;
   bottom: 0;
   left: 0;
   width: 100%;
   margin: auto;
   display: table;
   text-align: center;
   line-height: initial;
}

/**页脚**/
.notContent{padding: 15px 0;text-align: center;}
.pageChange {
    text-align: center;
    margin-top: 18px;
}
.pageChange .pageChange-span {
    border: 1px solid #bfbfbf;
    height: 25px;
    line-height: 25px;
    text-align: center;
    font-size: 12px;
    vertical-align: top;
}
.pageChange .pageChange-span2{
    width: 25px;
    cursor: default;
}

.pageChange .pageChange-span1, .pageChange .pageChange-span2 {
    margin-right: 17px;
}

.pageChange .pageChange-span1, .pageChange .pageChange-span3 {
    width: 60px;
    cursor: pointer;
}
/**按钮样式**/

.pageChange .pageChange-span-up,.pageChange .pageChange-span-up.active,
.pageChange .pageChange-span-down,.pageChange .pageChange-span-down.active{
    display: inline-block;
    width: 8px;
    height: 10px;
    background: url(../imgs/dataManagement/dataManagement.png) no-repeat ;
}
.pageChange .pageChange-span-up{
    margin-right:4px
}
.pageChange .pageChange-span-down{
    margin-left:4px
}
/**左侧按钮禁用**/
.pageChange .pageChange-span-up{
    background-position:-41px -35px;
}
/**左侧按钮激活**/
.pageChange-span1.active .pageChange-span-up{
    background-position: -23px -35px;
}

/**右侧按钮禁用**/
.pageChange .pageChange-span-down{
    background-position:-59px -35px;
}
/**右侧按钮激活**/
.pageChange-span3.active .pageChange-span-down{
    background-position: -5px -35px;
}
.pageChange-span1.disabled,.pageChange-span3.disabled{
    color:#d2d2d2;
    cursor:not-allowed;
}


.pageChange .txt {
    font-size: 12px;
    margin: 0 8px;
    cursor: default;
    vertical-align:middle;
}



/*弹窗- 新建测站信息*/
#createStation .modal-content{
    border-radius: 3px;
    width: 500px;
    margin-left: 50px;
    font-size: 14px;
}
#createStation .modal-header{
    padding: 9px 15px;
    border-bottom: 1px dashed #e5e5e5;
}
#createStation .modal-body{
    padding: 18px;
    color: #4e697c;
}
#createStation form>p{
    margin-bottom: 20px;
}
#createStation form>p>span{
    display: inline-block;
    width: 126px;
    text-align: right;
    padding-right: 16px;

}
#createStation form>p>input,
#createStation form>p .select-picker{
    width: 258px;
    height: 26px;
    border: 1px solid #e1e1e1;
    border-radius: 3px;
    text-indent: 10px;
    outline: none;
}
#createStation form>p .select-picker button{
    height: 24px;
    padding: 0;
    border: none;
}
#createStation form>p .select-picker button .filter-option{
    text-indent: 10px;
}
.bootstrap-select.btn-group .dropdown-menu li a span.text{
    display: block;
}
#createStation form>p>textarea{
    width: 257px;
    height: 79px;
    vertical-align: top;
    border: 1px solid #e1e1e1;
    border-radius: 3px;
    padding: 10px;
}
