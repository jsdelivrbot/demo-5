#container {
    background: #F0F6F6;
    width: 100%;
}
#left {
    width: 210px;
    background-color: #374050;
    float: left;
}
#left .left-item {
    height: 81px;
    line-height: 81px;
    color: #A9ACB1;
    text-align: center;
    font-size: 16px;
    cursor: pointer;
}
#left .left-item.active {
    color: #fff;
    background: #5D6371;
}
#right {
	margin-left:20px;
	margin-top:20px;
    padding:26px 40px;
    width: calc(100% - 250px);
    // height: calc(100% - 135px);
    float: left;
    background: #fff;
}
















