    margin-right:4px
}
.pageChange .pageChange-span-down{
    margin-left:4px
}
/**左侧按钮禁用**/
.pageChange .pageChange-span-up{
    background-position:-41px -35px;
}
/**左侧按钮激活**/
.pageChange-span1.active .pageChange-span-up{
    background-position: -23px -35px;
}

/**右侧按钮禁用**/
.pageChange .pageChange-span-down{
    background-position:-59px -35px;
}
/**右侧按钮激活**/
.pageChange-span3.active .pageChange-span-down{
    background-position: -5px -35px;
}
.pageChange-span1.disabled,.pageChange-span3.disabled{
    color:#d2d2d2;
    cursor:not-allowed;
}


