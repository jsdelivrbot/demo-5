}

/*若浏览器宽度小于1420px时*/
@media only screen and (max-width: 1420px) {
    .pannel-content,.right-body-table,.right-title{
        max-width:1325.5px;
        font-size:14px;
    }
    #content1Starttime,#content1Endtime{
        text-indent: 5px;
    }
    #right{
        padding: 0px 60px 20px 40px;
    }
    /*第1页处理*/
    #right .rightContent1 .right-title .create,
    #right .rightContent1 .right-title .confirm{
        width: 75px;
    }
    #right .rightContent1 .bootstrap-select > .dropdown-toggle.bs-placeholder{
        width: 160px;
    }
    #right .rightContent1 .selectpickerBox {
        margin-right: 26px;
    }
    #right .rightContent1 .right-table-title-item,
    #right .rightContent1 .right-table-row-item {
        width: calc((100% - 340px) / 3);
    }
    #right .rightContent1 .right-table-title-item:nth-child(2),
    #right .rightContent1 .right-table-title-item:nth-child(4),
    #right .rightContent1 .right-table-row-item:nth-child(2),
    #right .rightContent1 .right-table-row-item:nth-child(4){
        width: 170px;
    }
    #right .right-body-table .right-table-body li .editBox{
        margin:0 9px 0 0;
    }
    #right .right-body-table .right-table-body li .deleteBox {
        margin:0 0 0 9px;
    }
    /*第二页-新建测站*/
    #createStation .modal-dialog{
        width: 1100px;
    }
    #createStation .modal-body .mapBox{
        width:calc(100% - 410px);
        height:506px;
    }

}
@media only screen and (max-width: 1300px) {
    .bootstrap-select:not([class*="col-"]):not([class*="form-control"]):not(.input-group-btn){
        width: 145px;
    }

    /*宽度过窄,标题内容溢出 start*/
    #right .rightContent3 .right-table-title-item:nth-child(1), #right .rightContent3 .right-table-row-item:nth-child(1){
        width: 12.5%;
    }
    #right .rightContent3 .right-table-title-item:nth-child(2), #right .rightContent3 .right-table-row-item:nth-child(2){
        width: 10%;
    }
    /*宽度过窄,标题内容溢出 end*/
}


@media only screen and (max-width: 1400px){
	#container{
		top: 96px;
	}
}
