#map{
	width:100%;
	height:calc(100vh - 135px);

}
.contentRight {
    width: 458px;
    position: absolute;
    top:135px;
    right: -7px;
    background: #fff;
    min-height: 200px;
    height:calc(100vh - 135px);
    border-radius: 3px;
    box-shadow: 1px 3px 2px 2px rgba(0, 0, 0, 0.2);
    transition: all 0.2s ease 0s;
}

.contentRighthomei {
    cursor: pointer;
    width: 40px;
     height: 42px;
    text-align: center;
    border-right: 1px solid #312e28;
    float: right;
    padding-top: 15px;
   
}
.contentRightTabs {
	background:#616266;
    padding-left: 10px;
    height: 49px;

}
.contentRightTabs .tabs{
	display: inline-block;
	height: 43px;
    line-height: 43px;
    width: 25%;
    margin-top:6px;
    text-align: center;
    font-size: 18px;
    color:#fff;
    cursor:pointer;

}
.contentRightTabs .tabs.active{
	background:#fff;
	color:#000;
}
.tab-pannel{
	display:none;
}
.tab-pannel.active{
	display:block;
}