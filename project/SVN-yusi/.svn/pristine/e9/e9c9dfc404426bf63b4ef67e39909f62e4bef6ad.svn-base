/*表格样式*/
.container-bottom-content{
    border: 1px solid #e1e1e1;
}
.container-bottom-content ul{
    font-size: 0px;
    width: 100%;
    height: 50px;
    line-height: 50px;

}
.container-bottom-content>ul{
    background-color: #f2f2f2;
}
.container-bottom-content ul li{
    display: inline-block;
    font-size: 14px;
    text-align: center;
    height: 100%;
    width: calc((100% - 69px) / 6);
    border-right: 1px solid #e1e1e1;
    border-bottom: 1px solid #e1e1e1;
    vertical-align: middle;
}
.container-bottom-content ul li:last-child{
    border-right: none;
}
.container-bottom-content ul li:first-child{
    width: 69px;
}
.container-bottom-content div ul{
    height: 56px;
}
.container-bottom-content div ul li{
    padding: 3px 5px;
}
.container-bottom-content div ul:last-child li{
    border-bottom: none;
}
.container-bottom-content input{
    width: 100%;
    height: 46px;
    line-height: 46px;
    text-align: center;
    border: 1px dashed transparent;
    background-color: #fff;
}
.container-bottom-content.active input{
    border: 1px dashed #4b93e8;
    background-color: #f0f7ff;
}
.container-bottom-content.active input::-ms-clear{display:none;}

#scrollBox{
    max-height: 400px;
}
.mCSB_inside > .mCSB_container{
    margin-right: 0;
}

/**按钮样式**/
#gotoToday:hover,
#gotoToday:active,
.save:hover,
.save:active,
.edit:hover,
.edit:active{
    cursor:pointer;
    background:#4385d5!important;
}



/*顶部日期条的宽度*/
@media screen and (max-width:1366px){
    .container-top p{
        margin-bottom: 20px;
    }
    .sliderBar{
        height:90px;
    }
    .sliderBar .showBox{
        max-width: calc(50px * 22);
    }
    .sliderBar li{
        padding: 18px 5px 14px;
    }
}
@media screen and (min-width:1360px) and (max-width:1600px){
    .sliderBar .showBox{
        max-width: calc(50px * 24);
    }
}
@media screen and (min-width:1600px) and (max-width:1800px){
    .sliderBar .showBox{
        max-width: calc(50px * 28);
    }
}
@media screen and (min-width:1800px) and (max-width:1920px){
    .sliderBar .showBox{
        max-width: calc(50px * 31);
    }
}
