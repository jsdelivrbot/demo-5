#container{
    min-width: 1260px;
    position: absolute;
    top: 146px;
    left: 0;
    right:0;
    bottom: 0;
}

.container-top {
    background: #fff;
    margin-top: 10px;
    padding-top: 32px;
    box-shadow: 1px 10px 16px #E5EFFB;
	text-align: center;
}

.container-top p {
    margin-bottom: 19px;
}

.container-top .title {
    margin-left: 28px;
}
.wrap-title-third {
    display:inline-block;
}
.prevButton, .nextButton {
    display: inline-block;
    vertical-align: middle;
    width: 10px;
    height: 10px;
    background: url(../imgs/common/common.png) no-repeat;
    margin: 0 0 2px 0;
    background-clip: content-box;
    box-sizing: content-box;
    padding: 15px 15px;
}
.prevButton.active {
    background-position: -64px -126px;
    cursor: pointer;
}
.nextButton.active {
    background-position: -81px -126px;
    cursor: pointer;
}
.prevButton {
    background-position: -98px -126px;
}
.nextButton {
    background-position: -119px -126px;
}
.timePicker {
    width: 121px;
    height: 38px;
    text-align: center;
    font-size:16px;
    font-weight:bold;
    color:#4791EA;
    border:none;
    outline: none;
    cursor: pointer;
}

.container-top .circle {
    display: inline-block;
    width: 12px;
    height: 12px;
    line-height: 12px;
    text-align: center;
    border-radius: 50%;
    margin-right: 8px;
    vertical-align: middle;
}

.container-top .circle.red {
    background: #E7211E;
}

.container-top .circle.green {
    background: #01CF60;
}

.container-top .circle.yellow {
    background: #FFDE01;
}

.container-top .circle.gray {
    background: #ccc;
}

.top-right-last {
    margin-right: 42px;
}

.container-top button {
    width: 90px;
    height: 32px;
    line-height: 32px;
    text-align: center;
    color: #fff;
    background: #4991EA;
    border-radius: 3px;
    border: none;
    font-size: 14px;
    margin-left: 28px;
}

.left, .right {
    display: inline-block;
    width: 51px;
    vertical-align: middle;
    margin: 0 10px;
}

.left span, .right span {
    display: inline-block;
    width: 44px;
    height: 44px;
    border-radius: 50%;
    background: url('../imgs/common/common.png') no-repeat;
}

.left span.leftBtn.active {
    background-position: -171px -56px;
    cursor: pointer;
}

.left span.leftBtn {
    background-position: -171px -107px;
}

.right span.rightBtn.active {
    background-position: -233px -56px;
    cursor: pointer;
}

.right span.rightBtn {
    background-position: -233px -107px;
}

.sliderBar {
    display: inline-block;
    vertical-align: middle;
    /*width: calc(100% - 102px);*/
	max-width: 1600px;
	height: 110px;
	overflow: hidden;
	text-align: center;
    position: relative;
}
.sliderBar .showBox{
    overflow: hidden;
    text-align: center;
    height: 100%;
    display: inline-block;
    position: relative;
}
.sliderBar ul{
	text-align: left;
    height: 100%;
    width: calc(50px * 31);
    position: relative;
    left: 0px;
    /*transition: left .25s;*/
}

.sliderBar li {
	font-size: 0;
    position: relative;
    display: inline-block;
    width: 50px;
    height: 100%;
    text-align: center;
    cursor: pointer;
	padding: 28px 5px 14px;
}
.sliderBar li.curr{/*当前选中*/
    background:#d2d2d2;
}
.sliderBar li.active {/*当前日期*/
    background-color: #4791ea;
    cursor: pointer;
    color: #fff;
}


.sliderBar li .dateDay{  /*正常报*/
	font-size: 14px;
	width: 100%;
    height: 0;
	border-radius: 50%;
    padding-bottom: 100%;
    background: #00cf63;
    position: relative;
}

.sliderBar li.lateReport .dateDay{/*迟报*/
    background-color: #ffde00;
}
.sliderBar li.leakReport .dateDay{/*漏报*/
    background-color: #e7211e;
}
.sliderBar li.notReport .dateDay{/*未报*/
    background-color: #ccc;
}
.sliderBar li.notReport .dateDay span{
    color:#999;
}

.sliderBar li.active .dateDay{
    background-color: #fff;
}
.sliderBar li>div:last-child{
	font-size: 14px;
    margin-top: 5px;
}
.sliderBar li.notReport>div:last-child{
    color:#ccc;
}

.sliderBar li>div.weekend{
	color: #8f9ca5;
}

.sliderBar li span {
	width: 20px;
    height: 20px;
    position: absolute;
    top: 50%;
    left: 50%;
    margin-top: -10px;
    margin-left: -10px;
	color: white;
}

.sliderBar li.active span {
    color: #4791ea;
}

.sliderBar li.active div[data-bind=weekDayTip]{
    color: white;
}

.container-bottom{
    padding:60px 170px 20px 170px;
    margin-top:14px;
    background:#fff;
    height:calc(100% -  220px);
}
.container-bottom-title{
    margin-bottom:26px;
    height: 32px;
}
.container-bottom-title>p{
    vertical-align: sub;
}
.container-bottom-title button{
    color: #fff;
    background-color: #fff;
    border-radius: 3px;
    border: 1px solid #4991EA;
    color: #4991EA;
    font-size: 14px;
    width: 88px;
    height: 32px;
    line-height: 32px;
}
.container-bottom-title button.save,
.container-bottom-title button.edit{
    background: #4991EA;
    color: white;
    margin-right: 10px;
}
.ui-jqgrid-labels{
    background:#f2f2f2;
}

