#wrap {
    width: 100%;
    min-width:1200px;
    padding: 0 42px;
    background-color: white;
    position: absolute;
    top: 145px;
    left: 0;
    right:0;
    bottom: 0;
}
.wrap-title {
    padding: 25px 0;
    text-align: center;
    position: relative;
}
/*.wrap-title>div {
    font-size: 0;
}*/

.wrap-title-third {
    display: inline-block;
}
.wrap-title-forth{
    position:absolute;
    top:36px;
    right:0;
}

.prevButton, .nextButton {
    display: inline-block;
    vertical-align: middle;
    width: 10px;
    height: 10px;
    background: url(../imgs/common/common.png) no-repeat;
    margin: 0 0 2px 0;
    background-clip: content-box;
    box-sizing: content-box;
    padding: 15px 15px;
}

.prevButton {
    margin-right: -8px;
    background-position: -98px -126px;
}
.nextButton {
    background-position: -119px -126px;
}
.prevButton.active {
    background-position: -64px -126px;
    cursor: pointer;
}
.nextButton.active {
    background-position: -81px -126px;
    cursor: pointer;
}
.timePicker {
    width: 121px;
    height: 38px;
    text-align: center;
    text-indent: 5px;
    font-size:16px;
    font-weight:bold;
    color:#4991EA;
    border:none;
    outline: none;
    cursor: pointer;
}


.circle{
	display:inline-block;
	border-radius: 50%;
	width:8px;
	height:8px;
}

.circle.normal{
	background:#01CF62;
}
.circle.abnormal{
	background:#FEDD01;
}
.circle.bad{
	background:#E7211B;
}


.ui-jqgrid-btable tr.jqgrow td:nth-child(1){
    overflow: hidden;
    white-space: nowrap;
    text-overflow : ellipsis;
}
