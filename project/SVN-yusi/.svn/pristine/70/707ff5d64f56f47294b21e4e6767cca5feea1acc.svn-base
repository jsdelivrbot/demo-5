#wrap{
    width:100%;
    min-width:1200px;
    padding: 0 42px;
    background-color: white;
    position:absolute;
    top:145px;
    left:0;
    bottom:0;

}

/*wrap中top内容*/
.wrap-title{
    padding: 22px 0;
    text-align: center;
    position: relative;
    height:86px;
}
.wrap-title>div{
    font-size: 0;
}
.wrap-title>div>*{
    font-size: 12px;
}
.wrap-title-first,
.wrap-title-second,
.wrap-title-forth{
    position: absolute;
    top: 25px;
}
.wrap-title-first{
    margin-right: 40px;
    left: 0;
}
.wrap-title-second{
    left: 259px;
}
.wrap-title-third{
    display: inline-block;
}
.wrap-title-forth{
    right: 0;
    top: 26px;
}
.wrap-content{
    max-width: 1780px;
    margin: 0 auto;
    font-size: 14px;
    /*transition: opacity 0.1s;*/
}
.wrap-title button{
    padding: 9px 17px;
    background-color: white;
    border: 1px solid #ccc;
    color: #999;
}
.wrap-title button#months{
    border-left:none;
    border-right:none;
}
.wrap-title button.active,.wrap-title-forth button{
    color: white;
    background-color: #5db75d;
    border: 1px solid #5db75d;
}
.wrap-title-forth button{
    width: 90px;
}
.wrap-title .wrap-title-first button.active{
    background-color: #4791ea;
    border: 1px solid #4791ea;
}
.wrap-title-first button:first-child,
.wrap-title-second button:first-child,
.wrap-title-forth button:first-child{
    border-top-left-radius: 2px;
    border-bottom-left-radius: 2px;
}
.wrap-title-first button:last-child,
.wrap-title-second button:last-child,
.wrap-title-forth button:last-child{
    border-top-right-radius: 2px;
    border-bottom-right-radius: 2px;
}
.prevButton,.nextButton{
    display:inline-block;
    vertical-align: middle;
    width:10px;
    height:10px;
    background:url('../imgs/common/common.png') no-repeat;
    margin: 0 0 2px 0;
    background-clip: content-box;
    box-sizing: content-box;
    padding: 15px 15px;
}
.prevButton{
    background-position: -98px -126px;
}
.nextButton{
    background-position: -119px -126px;
}
.prevButton.active{
    background-position: -64px -126px;
    cursor:pointer;
}
.nextButton.active{
    background-position: -81px -126px;
    cursor:pointer;
}

.timePicker {
    width: 121px;
    height: 38px;
    text-align: center;
    text-indent: 5px;
    font-size:16px;
    font-weight:bold;
    color:#4991EA;
    border:none;
    outline: none;
    cursor: pointer;
}



/*限制grid高度*/
.ui-jqgrid-bdiv{
    max-height: calc(100vh - 320px) !important;
}
/*解决grid高度错乱问题*/
div.frozen-bdiv{
    /*height: auto !important;*/
}
/*文字过多添加省略号*/
.ui-jqgrid-btable tr.jqgrow td:nth-child(1),
.ui-jqgrid-btable tr.jqgrow td:nth-child(2){
    overflow: hidden;
    white-space: nowrap;
    text-overflow : ellipsis;
    text-align: left !important;
}
/*设置grid滚动条*/
.ui-jqgrid-bdiv::-webkit-scrollbar {
    width: 8px;
    height: 8px;
    border-radius: 10px;
    background-color: rgba(0,0,0,0.1);
    box-shadow: inset 1px 0 1px rgba(0,0,0,0.1);
}

.ui-jqgrid-bdiv::-webkit-scrollbar-thumb{
    background-color: #ced5e3;
    border-radius: 10px;
}
.ui-jqgrid-bdiv::-webkit-scrollbar-thumb:hover{
    /*background-color: rgba(0,0,0,.6);*/
}
