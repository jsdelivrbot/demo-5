#container {
    background: #343D4E;
    width: 100%;
}
#left {
    width: 185px;
    background-color: #374050;
    position:absolute;
    height:auto;
}
#left .left-item {
    height: 81px;
    line-height: 81px;
    color: #A9ACB1;
    text-align: center;
    font-size: 16px;
    cursor: pointer;
}
#left .left-item.active {
    color: #fff;
    background: #5D6371;
}
#right {
	margin-left:185px;
    padding:20px;
    min-height: 760px;
    background:#EFF6F6;
    
}

#right .right-title{
    text-align:center;
    background:#fff;
    padding:30px 40px;
    margin-bottom:20px;
}
#right  .right-title .btn-tabs button {
    padding:9px 15px;
    color: #fff;
    background-color: #fff;
    border-radius: 3px;
    border: 1px solid #4991EA;
    color: #4991EA;
    font-size:14px;
}
#right .right-title .btn-tabs button.active {
    background-color: #4991EA;
    color: #fff;
}
#right .right-title .export {
    padding:9px 46px;
    color: #fff;
    background-color: #5CB85C;
    border-radius: 3px;
    border: none;
    font-size: 14px;
}
.selectBox {
    display:inline-block;
    position:relative;
}

.prevButton, .nextButton {
    -webkit-user-select: none;
    -moz-user-select: none;
    -ms-user-select: none;
    user-select: none;
    display: inline-block;
    width: 8px;
    height: 10px;
    background: url(../imgs/common/common.png) no-repeat;
    cursor: pointer;
}
.prevButton {
    background-position: -80px -141px;
}
.nextButton {
    background-position: -95px -141px;
}
.selectView{
    display:inline-block;
    vertical-align: middle;
    width: 121px;
    height: 38px;
    line-height:38px;
    text-align: center;
    text-indent: 5px;
    font-size: 16px;
    color: #4991EA;
    cursor:pointer;
  
}
.selectBox .dropDonMenu{
    display:none;
    position: absolute;
    top:40px;
    left:28px;
    width:130px;
    height: 210px;
    background:#fff;
    box-shadow: 1px 1px 1px #ddd;
}
.selectBox .dropDonMenu li{
    line-height:40px;
}
.selectBox .dropDonMenu li:hover{
    cursor:pointer;
}
.selectBox .dropDonMenu li:hover,
.selectBox .dropDonMenu li:active{
    background:#4991EA;
    color:#fff;
}
.rightContent{

}
.rightContent .echart,.rightContent .echartContent{
    width:817px;
    height:670px;
    margin-right:18px;
    background-color:#fff;

}
.rightContent .rightTable{
    width:670px;
    height:670px;
    background-color:#fff;
}
table tbody tr{line-height:50px;}
table tbody tr td{text-align:center;border-top:none!important;line-height:60px!important;}
.table>thead>tr>th{border-bottom:none!important;}