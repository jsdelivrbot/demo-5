.btn {
    padding: 0;
    margin: 0;
}
.modal-dialog {
    margin: 0;
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate(-50%,-50%)!important;
    z-index: 1;
}
/*弹窗- 增补-测报数据*/
 #augmentModal .modal-dialog {
    width: 502px;
}
#augmentModal .modal-header {
    min-height: 16.43px;
    padding: 0;
    padding-left: 30px;
    height: 40px;
    line-height: 40px;
    color: #696969;
    font-size: 14px;
    border-bottom: 1px dashed #bfbfbf;
}
#augmentModal .modal-header .closeImg {
    margin-right: 10px;
    margin-top: 6px;
    outline: none;
}
#augmentModal .modal-body {
    padding: 0;
    margin: 25px 92px 25px 72px;
    font-size: 14px;
}
#augmentModal .modal-body-item {
    line-height: 26px;
    margin-bottom: 23px;
    color:#999;
}
#augmentModal .modal-body-item .modal-body-item-name {
    width: 83px;

}
#augmentModal .modal-body-item input {
    width: 248px;
    height: 26px;
    border: 1px solid #E1E1E1;
    border-radius: 3px;
    text-indent:10px;
}

#augmentModal .modal-footer {
    padding:0;
    padding-bottom: 15px;
    border-top: none;
    text-align: center;
}
#augmentModal .modal-footer .btn-keep, .btn-delete, .btn-delete:hover, .btn-keep:hover, .btn-keep:active, .btn-delete:active{
    background-color: #4A91EA;
    border: none;
    color: #fff;
    padding: 0 10px!important;
    height: 22px;
    line-height: 22px;
    text-align: center;
    border-radius: 5px;
    font-size: 12px;
}
#augmentModal .modal-footer .btn-cancel {
    padding: 0 10px!important;
    height: 22px;
    line-height: 21px;
    text-align: center;
    border-radius: 5px;
    font-size: 12px;
    background-color: #eeeeee;
    border: 1px solid #bfbfbf;
    margin-left:5px;
}
/*弹窗- 新建测站信息*/
#createStation .modal-content{
    border-radius: 3px;
    width: 500px;
    margin-left: 50px;
    font-size: 14px;
}
#createStation .modal-header{
    padding: 9px 15px;
    border-bottom: 1px dashed #e5e5e5;
}
#createStation .modal-body{
    padding: 18px;
    color: #4e697c;
}
#createStation form>p{
    margin-bottom: 20px;
}
#createStation form>p>span{
    display: inline-block;
    width: 126px;
    text-align: right;
    padding-right: 16px;

}
#createStation form>p>input,
#createStation form>p .select-picker{
    width: 258px;
    height: 26px;
    border: 1px solid #e1e1e1;
    border-radius: 3px;
    text-indent: 10px;
    outline: none;
}
#createStation form>p .select-picker button{
    height: 24px;
    padding: 0;
    border: none;
}
#createStation form>p .select-picker button .filter-option{
    text-indent: 10px;
}
.bootstrap-select.btn-group .dropdown-menu li a span.text{
    display: block;
}
#createStation form>p>textarea{
    width: 257px;
    height: 79px;
    vertical-align: top;
    border: 1px solid #e1e1e1;
    border-radius: 3px;
    padding: 10px;
}
/*#createStation form>p .select-picker{
    width: 258px;
    height:
}*/
#createStation .saveAndQuit{
    text-align: center;
}
#createStation p.saveAndQuit>input{
    width: 64px;
    height: 24px;
    border-radius: 6px;
    text-align: center;
    text-indent: 0;
    font-size: 12px;
    margin: 0 3px;
}
#createStation .saveAndQuit>input:first-child{
    background-color: #4b90ed;
    color: white;
}
#createStation .saveAndQuit>input:last-child{
    background-color: #eee;
    color: #696969;
}


/*弹窗-区域平均降雨弹窗*/
.modal-backdrop.in{
    opacity: .3;
}
#averageRainfall .modal-content,
#averageRainfallModify .modal-content{
    border-radius: 3px;
    color: #50697a;
    font-size: 14px;
    width: 800px;
    margin-left: 5px;
    margin-top: 88px;
    text-align: center;
}
#averageRainfall .modal-header,
#averageRainfallModify .modal-header{
    padding: 8px 15px;
    border-bottom: 1px dotted #bfbfbf;
}
#averageRainfall .modal-body{
    border: 18px 28px;
    height: 530px;
    overflow: auto;
}
#averageRainfall .modal-body-content{
    border: 1px solid #e1e1e1;
}
#averageRainfall .modal-body-content>ul{
    border-bottom: 1px solid #e1e1e1;
}
#averageRainfall .modal-body-content>ul:last-child{
    border-bottom: none;
}
#averageRainfall .modal-body-content>ul>li{
    display: inline-block;
    width: 13%;
    height: 86px;
    margin: 0 8px;
    line-height: 86px;
    vertical-align: top;
}
#averageRainfall .modal-body-content>ul>li span{
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}
#averageRainfall .modal-body-content>ul>li.modify i,
#averageRainfall .modal-body-content>ul>li.delete i{
    display: inline-block;
    width: 20px;
    height: 20px;
    vertical-align: middle;
    cursor: pointer;
    margin-right: 5px;
}
#averageRainfall .modal-body-content>ul>li.modify,
#averageRainfall .modal-body-content>ul>li.delete{
    width: 10%;
    color: #599bec;
    margin: 0;
    cursor: pointer;
}
#averageRainfall .modal-body-content>ul>li.delete{
    margin-right: 8px;
}
#averageRainfall .modal-body-content>ul>li.modify i{
    background: url(../imgs/dataManagement/dataManagement.png) no-repeat -5px -5px;
}

#averageRainfall .modal-body-content>ul>li.delete i{
    background: url(../imgs/dataManagement/dataManagement.png) no-repeat -35px -5px;
}

#averageRainfall .modal-body-content>ul>li>div{
    position: relative;
    top: 50%;
    transform: translateY(-50%);
}
#averageRainfall .modal-body-content>ul>li>div>span{
    display: block;
    line-height: normal;
}

