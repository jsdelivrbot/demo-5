<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
    "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatisPro.myBatisDao.WaterStorRelaMapper">
	<cache type="mybatisPro.mybatisRedis.MybatisRedisCache" />
	
	<!-- 查询测站ID -->
	<select id="getStorageCapacity" resultType="float">
		SELECT storageCapacity FROM waterStorageRelastionship WHERE waterLevel=#{waterLevel}
	</select>
	
</mapper>