<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC 
    "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mybatisPro.myBatisDao.WzRiverRsvrMapper">

	<!-- 水库水位数据导入 -->
	<insert id="impRsvDataInfo"  parameterType="java.util.List" >
		 INSERT IGNORE INTO ST_WZ_RSVR_R(
			`sta_id`, `time`, `waterLevel`, `storageCapacity`, `createtime`
		)
		VALUES 
		<foreach collection="data_info" index="index" item="item"  separator="," >
			(
			 	#{item.sta_id},#{item.time},#{item.waterLevel},#{item.storageCapacity},#{item.createtime}
			)
		</foreach>
	</insert>
	
	<!-- 河道水位数据导入 -->
	<insert id="impRiverDataInfo"  parameterType="java.util.List" >
		 INSERT IGNORE INTO ST_WZ_RIVER_R(
			`sta_id`, `time`, `waterLevel`, `rateOfFlow`, `createtime`
		)
		VALUES 
		<foreach collection="data_info" index="index" item="item"  separator="," >
			(
			 	#{item.sta_id},#{item.time},#{item.waterLevel},#{item.rateOfFlow},#{item.createtime}
			)
		</foreach>
	</insert>

	
</mapper>